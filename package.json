{
  "name": "yannic.world",
  "version": "0.0.1",
  "description": "soonTM",
  "author": "James Harris <wopian@wopian.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "babel-node --presets env,stage-0  build/dev-server.js",
    "build": "babel-node --presets env,stage-0  build/build.js",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.vue client test/unit/specs",
    "pull": "git stash && git pull --rebase origin master && git stash pop"
  },
  "main": "client/main.js",
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 3.10.8"
  },
  "dependencies": {
    "axios": "~0.16.1",
    "es6-promise": "~4.1.0",
    "eventsource-polyfill": "~0.9.6",
    "friendly-errors-webpack-plugin": "~1.6.1",
    "front-matter": "~2.1.2",
    "function-bind": "~1.1.0",
    "marked": "~0.3.6",
    "offline-plugin": "~4.6.2",
    "timeago.js": "~3.0.1",
    "vue": "~2.2.6"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.7",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-eslint": "~7.2.1",
    "babel-loader": "~6.4.1",
    "babel-plugin-istanbul": "~4.1.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.3.3",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "chalk": "~1.1.3",
    "compression-webpack-plugin": "~0.4.0",
    "connect-history-api-fallback": "~1.3.0",
    "copy-webpack-plugin": "~4.0.1",
    "css-loader": "~0.28.0",
